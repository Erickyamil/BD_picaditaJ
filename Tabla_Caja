DROP TABLE IF EXISTS Caja;
CREATE TABLE Caja (
   caja_id INT(5) NOT NULL AUTO_INCREMENT PRIMARY KEY,                                 
   saldo_caja DECIMAL(10, 2) NOT NULL DEFAULT 0);
   
