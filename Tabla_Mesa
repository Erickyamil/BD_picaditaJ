CREATE TABLE mesa (
    mesa_id INT AUTO_INCREMENT NOT NULL PRIMARY KEY,
    numero_mesa VARCHAR(3) NOT NULL,
    total_personas INT NOT NULL,
    sucursal_id INT,
    FOREIGN KEY (sucursal_id) REFERENCES sucursal(sucursal_id)
);

